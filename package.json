  "devDependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "@importantimport/tsconfig": "^1.0.0-beta.2",
    "@types/node": "^22.14.0",
    "builtin-modules": "^5.0.0",
    "bumpp": "^10.1.0",
    "changelogithub": "^13.13.0",
    "eslint": "^9.23.0",
    "oxlint": "^0.16.4",
    "rolldown": "1.0.0-beta.7",
    "typescript": "^5.8.2",
    "unplugin-isolated-decl": "^0.13.6"
  }
