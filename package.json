{
  "name": "hfup",
  "type": "module",
  "version": "0.5.0",
  "description": "A collection of tools to help you deploy, bundle HuggingFace Spaces and related assets with ease.",
  "keywords": [],
  "packageManager": "pnpm@10.12.1",
  "author": "Moeru AI",
  "license": "MIT",
  "homepage": "https://github.com/moeru-ai/hfup",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moeru-ai/hfup.git"
  },
  "exports": {
    ".": {
      "types": "./dist/vite/index.d.ts",
      "import": "./dist/vite/index.mjs",
      "require": "./dist/vite/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite/index.d.ts",
      "import": "./dist/vite/index.mjs",
      "node": "./dist/vite/index.cjs"
    }
  },
  "main": "./dist/vite/index.cjs",
  "module": "./dist/vite/index.mjs",
  "types": "./dist/vite/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "rolldown -c",
    "typecheck": "tsc --noEmit",
    "lint": "oxlint",
    "up": "taze -w -r -I -f && pnpm prune && pnpm dedupe"
  },
  "dependencies": {
    "defu": "^6.1.4",
    "gray-matter": "^4.0.3",
    "vite": "^6.3.5"
  },
  "devDependencies": {
    "@importantimport/tsconfig": "^1.0.0-beta.2",
    "@types/node": "^22.15.31",
    "builtin-modules": "^5.0.0",
    "bumpp": "^10.1.1",
    "changelogithub": "^13.15.0",
    "oxlint": "^0.16.12",
    "rolldown": "^1.0.0-beta.16",
    "taze": "^19.1.0",
    "typescript": "^5.8.3",
    "unplugin-isolated-decl": "^0.13.11"
  }
}
